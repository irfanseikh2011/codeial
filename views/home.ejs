<link rel="stylesheet" href="/css/home.css">
<main>
<div id="container">
<h1><%= title %></h1>
<section id ="feed-posts">
    <h2>Posts</h2>
    <form action="/posts/create" id="new-post-form" method="POST">
        <textarea placeholder="Type here.." name="content" cols="40" rows="5"></textarea><br>
        <input class="but2"  type="submit" value="Post">
    </form>
</section>


<div id ="posts-list-container">
    <ul>
        <% for(post of posts) { %>
            <%- include('_post') -%>
        <%  } %>
    </ul>
</div>
</div>
</main>
 